<div class='box1'>	
	<div class="container">
		<% if @error %>
		<%= @error %>
		<%end%>
		<%= form_for :session, url: login_path do |f| %>
			<%= f.label :email %>
			<br/>
			<%= f.email_field :email %>
			<br/>
			<%= f.label :password %>
			<br/>
			<%= f.password_field :password %>
			<br/>
			<%= f.submit "Log in" %>
		<% end %>
		<%= link_to 'Home',welcome_home_path %>
	</div>
</div>
