<h1> New item </h1>
<%= form_for :product,url: products_path do |f| %>
	<%= f.label :Name%>
	<%= f.text_field :name%>
	<%= f.label :Numbers%>
	<%= f.number_field :num%>
	<%= f.label :Price%>
	<%= f.number_field :price,step: 'any'%>
	<%= f.submit :Create %>
<%end%>
<%= link_to "Home",welcome_home_path %>
