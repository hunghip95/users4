<div class='box1'>	
	<div class="container">
		<h1>Edit your phone</h1>
		<%= form_for @phone,url: phone_path(@phone),html: { multipart: true } do |f| %>
			<%= f.label :image%>
			<%= f.file_field :image%>
			<%= f.label :name %>
			<%= f.text_field :name %>
			<%= f.label :price %>
			<%= f.number_field :price %>
			<%= f.submit :Edit %>
		<%end%>
	</div>
</div>
