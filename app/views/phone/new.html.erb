<div class='box1'>	
	<div class="container">
		<h1>Your new phone</h1>
		
		<%= form_for :phone,url: product_phone_index_path, html: { multipart: true } do |f| %>
			<%= f.label :image%>
			<%= f.file_field :image%>
			<%= f.label :name %>
			<%= f.text_field :name %>
			<%= f.label :Price %>
			<%= f.number_field :price, :step => 0.1%>
			<%= f.submit :Create%>
		<%end%>
		<%= link_to 'Home',welcome_home_path %>
	</div>
</div>
